@model IEnumerable<OnlineContestSystem.Models.Ask>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChallengeTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ChallengeQuestion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AskDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChallengeTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ChallengeQuestion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AskDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
